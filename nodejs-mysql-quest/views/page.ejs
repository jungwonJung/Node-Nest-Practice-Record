<!DOCTYPE html>
<html>
<head>
    <title>리스트보기</title>
</head>
<body>
<h1>리스트보기</h1>
<a href="/">입력창</a>
<table border="1">
    <tr>
        <td>순서..</td>
        <td>id..</td>
        <td>des</td>
        <td>삭제</td>
        <td>수정</td>
    </tr>
    <%
    for(var i = (page * page_num) - page_num; i < (page * page_num); i++) {
    if(i > length){
        i++;
    }else{
        var data = results[i]
    %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= data.id %></td>
        <td><%= data.des %></td>
        <td><a href="/delete/<%= data.id %>">삭제</a></td>
        <td><a href="/update/<%= data.id %>">수정</a></td>
    </tr>
    <% }
    }
    %>
    <tr>
        <td colspan="6">
            <%
            for(var j = 0; j < results.length / page_num; j++){
            %>
            [<a href="/page/<%= j + 1 %>"><%= j + 1 %></a>]
            <%
            }
            %>
        </td>
    </tr>
</table>
<form action="/search" method="post">
    <table>
        <tr>
        <td><input type="text" name="id" placeholder="검색하기."></td>
        </tr>
    </table>
    <input type="submit" value="search">
</form>
</body>
</html>
